{
  "name": "web-authentication-demo",
  "version": "0.0.0",
  "description": "Node.js Web Authentication API demo.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "repository": "Rowno/web-authentication-demo",
  "license": "ISC",
  "private": true,
  "scripts": {
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "build": "yarn tsc && next build",
    "start": "node dist/src/server/boot.js",
    "ts-node": "ts-node --compiler-options '{\"module\":\"commonjs\"}'",
    "tsc": "tsc --module commonjs --outDir dist --target es2018 --noEmit false --isolatedModules false",
    "seed": "yarn ts-node scripts/seed-database.ts",
    "dev-typescript": "yarn tsc --watch --preserveWatchOutput",
    "dev-server": "PORT=3001 nodemon --delay 0.1 --watch dist dist/src/server/boot.js",
    "dev-next": "next dev --port 3002",
    "dev-proxy": "yarn ts-node scripts/dev-proxy.ts",
    "dev": "yarn tsc && concurrently -n typescript,server,next,proxy yarn:dev-typescript yarn:dev-server yarn:dev-next yarn:dev-proxy",
    "test-server": "NODE_ENV=test yarn start"
  },
  "engines": {
    "node": "^10"
  },
  "dependencies": {
    "@github/webauthn-json": "^0.3.6",
    "@hapi/joi": "^16.1.7",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "base64url": "^3.0.1",
    "cbor": "^5.0.1",
    "connect-redis": "^4.0.3",
    "cose-to-jwk": "^1.1.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.17.0",
    "helmet": "^3.21.1",
    "ioredis": "^4.14.1",
    "jwk-to-pem": "^2.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "next": "^9.1.1",
    "notistack": "^0.9.3",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "slonik": "^19.0.4"
  },
  "devDependencies": {
    "@types/cbor": "^2.0.0",
    "@types/connect-redis": "^0.0.11",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/hapi__joi": "^16.0.1",
    "@types/helmet": "^0.0.44",
    "@types/http-proxy": "^1.17.0",
    "@types/ioredis": "^4.0.18",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/lodash": "^4.14.144",
    "@types/node": "^10",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@types/slonik": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "concurrently": "^5.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-config-xo-typescript": "^0.19.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "http-proxy": "^1.18.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "eslintIgnore": [
    "/.next/",
    "/dist/"
  ],
  "eslintConfig": {
    "extends": [
      "xo/esnext",
      "xo-react",
      "xo-typescript",
      "prettier",
      "prettier/react",
      "prettier/@typescript-eslint"
    ],
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "react/prop-types": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/strict-boolean-expressions": "off",
      "@typescript-eslint/no-unnecessary-condition": "off"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
